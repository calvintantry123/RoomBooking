<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roombooking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoomBooking">
        <activity android:name=".View.Customer.RoomDetailActivity"></activity>
        <activity android:name=".View.Customer.EditActivity" />
        <activity android:name=".View.Customer.BookActivity" />
        <activity android:name=".View.Admin.ManageUserActivity" />
        <activity android:name=".View.Customer.CustHomeActivity" />
        <activity android:name=".View.Admin.AdminHomeActivity" />
        <activity android:name=".View.RegisterActivity" />
        <activity android:name=".View.MainActivity" />
        <activity android:name=".View.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>